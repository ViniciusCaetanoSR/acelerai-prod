<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Atribuição por Score</fullName>
        <active>false</active>
        <ruleEntry>
            <assignedTo>Lead_A</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Score__c</field>
                <operation>greaterOrEqual</operation>
                <value>70</value>
            </criteriaItems>
            <template>Acelerai_Folder/AtribuirLead</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Lead_B</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Score__c</field>
                <operation>lessOrEqual</operation>
                <value>69</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Score__c</field>
                <operation>greaterOrEqual</operation>
                <value>50</value>
            </criteriaItems>
            <template>Acelerai_Folder/AtribuirLead</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Lead_C</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Score__c</field>
                <operation>lessOrEqual</operation>
                <value>49</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Score__c</field>
                <operation>greaterOrEqual</operation>
                <value>30</value>
            </criteriaItems>
            <template>Acelerai_Folder/AtribuirLead</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Lead_D</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Score__c</field>
                <operation>lessOrEqual</operation>
                <value>29</value>
            </criteriaItems>
            <template>Acelerai_Folder/AtribuirLead</template>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
