<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opp_DataTerminoContrato</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL(StageName, &quot;Ganho&quot;),
ISBLANK(EndOfContractDate__c)
)</errorConditionFormula>
    <errorDisplayField>EndOfContractDate__c</errorDisplayField>
    <errorMessage>Preencha o campo &quot;Data de Termino do Contrato&quot; no objeto Oportunidade quando chegar na etapa &quot;Ganho&quot;.</errorMessage>
</ValidationRule>
